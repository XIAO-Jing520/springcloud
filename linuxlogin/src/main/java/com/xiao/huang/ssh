#!/bin/bash
n=(192.168.82.29 192.168.82.26 192.168.82.190 192.168.82.191 192.168.82.192 192.168.82.193 192.168.82.194 192.168.82.187 192.168.82.188 192.168.82.189 192.168.82.184 192.168.82.180 192.168.82.185)
uname="root"
pwd="dingjia@2017!@"
length=${#n[*]}
echo "$length"
for ((i = 0; i < $length; i++)); do
  echo "begin..."
  echo $i
  ./open.sh ${n[$i]} $uname $pwd
  echo "end..."
done

